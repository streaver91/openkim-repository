KIM_API_Version := 1.6.0

Unit_length := A
Unit_energy := eV
Unit_charge := e
Unit_temperature := K
Unit_time := ps

PARTICLE_SPECIES: 
$species spec 1

CONVENTIONS:
ZeroBasedLists  flag
Neigh_LocaAccess flag
Neigh_IterAccess flag
Neigh_BothAccess flag
#$iterloca flag
$nbc flag 

MODEL_INPUT:
numberOfParticles  integer  none  []
numberOfSpecies integer  none  []
particleSpecies  integer  none  [numberOfParticles]
